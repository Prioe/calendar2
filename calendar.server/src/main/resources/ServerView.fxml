<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>


<VBox spacing="5.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.malbertz.calendar.server.ui.GraphicalUI">
   <children>
      <TextArea fx:id="consoleArea" editable="false" prefHeight="345.0" prefWidth="371.0" />
      <HBox spacing="5.0">
         <children>
            <TextField fx:id="commandTextField" HBox.hgrow="ALWAYS" />
            <Button fx:id="sendButton" defaultButton="true" mnemonicParsing="false" text="Send" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
